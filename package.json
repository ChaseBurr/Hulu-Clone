{
     "homepage": "https://chaseburr.github.io/Hulu-Clone/",
     "name": "hulu-clone",
     "version": "0.1.0",
     "private": true,
     "dependencies": {
          "axios": "^0.21.1",
          "react": "^17.0.1",
          "react-dom": "^17.0.1",
          "react-router-dom": "^5.2.0"
     },
     "scripts": {
          "prod": "webpack",
          "dev": "yarn clean && webpack --config webpack.dev.js --mode=development && webpack serve",
          "webpack-dev": "webpack-dev-server",
          "clean": "rimraf ./build/*",
          "deploy": "gh-pages -d build",
          "predeploy": "npm run build"
     },
     "eslintConfig": {
          "extends": [
               "react-app",
               "react-app/jest"
          ]
     },
     "browserslist": {
          "production": [
               ">0.2%",
               "not dead",
               "not op_mini all"
          ],
          "development": [
               "last 1 chrome version",
               "last 1 firefox version",
               "last 1 safari version"
          ]
     },
     "devDependencies": {
          "@babel/core": "^7.12.13",
          "@babel/plugin-transform-runtime": "^7.12.15",
          "@babel/preset-env": "^7.12.13",
          "@babel/preset-react": "^7.12.13",
          "babel-loader": "^8.2.2",
          "css-loader": "^5.0.1",
          "dotenv-webpack": "^6.0.0",
          "gh-pages": "^3.1.0",
          "html-webpack-plugin": "^5.0.0",
          "sass": "^1.32.6",
          "sass-loader": "^10.1.1",
          "style-loader": "^2.0.0",
          "webpack": "^5.20.2",
          "webpack-cli": "^4.5.0",
          "webpack-dev-server": "^3.11.2"
     }
}
